query tpplcMasterProducts($input: TpplcMasterProductSearchInput!) {
  tpplcMasterProducts(input: $input) {
    edges {
      gpid
      name
      brandShared
      brands {
        id
      }
      products {
        id
      }
    }
    totalCount
    pageInfo {
      endCursor
      hasNextPage
    }
  }
}
